{
  "name": "squire",
  "version": "1.0.0",
  "description": "A javascript implementation of the squire COVID-19 model",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r esm",
    "test:watch": "nodemon --watch . --exec 'npm test || true'",
    "build:data": "Rscript ./R/export.R ./data && Rscript ./R/run.R ./data",
    "build:odin": "./build.sh explicit_SEIR_deterministic.R build/squire.js",
    "build": "npm run build:data && npm run build:odin && rollup -c >> build/squire.js",
    "e2e": "node -r esm e2e/test.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com/mrc-ide/squire_js"
  },
  "author": "Imperial College London",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "esm": "^3.2.25",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "rollup": "^2.6.1",
    "sinon": "^9.0.2"
  }
}
